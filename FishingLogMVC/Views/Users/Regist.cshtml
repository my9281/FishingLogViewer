
<div class="login user-regist-box">
	<h2>@ViewBag.Dic["Regist"]</h2>
	<div class="login-box">
		<div class="login-template">
			<div class="login-inner">
				<div>
					<span>@ViewBag.Dic["AccountID"]</span>
				</div>
				<input type="text" id="rid" placeholder="@ViewBag.Dic["AccountIDPH"]">
				<div>
					<span>@ViewBag.Dic["Password"]</span>
				</div>
				<input type="password" id="rpa" placeholder="@ViewBag.Dic["PasswordPH"]">		
				<div>
					<span>@ViewBag.Dic["RecheckPassword"]</span>
				</div>
				<input type="password" id="rrepa" placeholder="@ViewBag.Dic["RecheckPassword"]">
			</div>
			<div class="btn">
				<button class="login_btn" id="regist-btn">
					@ViewBag.Dic["Submit"]
				</button> 
			</div>
		</div> 
	</div>
</div>

<script type="text/javascript" src="~/js/users/regist.js"></script>
